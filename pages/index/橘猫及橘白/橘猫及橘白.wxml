<!--index.wxml-->

<block wx:for="{{catlist}}">
  <navigator url="/pages/cats/{{item.name}}/{{item.name}}" >
    <view class="catitem">
      <image  class = 'avator ' src="https://shtu.oss-cn-shanghai.aliyuncs.com/cats/{{item.name}}_circle.png" mode='aspectFill' ></image>
      <view class= "cat-name">{{item.name}}</view> 
    </view>
  </navigator>

  <view class="divLine"></view>
  
</block>
